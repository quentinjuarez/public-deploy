{
  "name": "public-deploy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "start:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "start:staging": "docker-compose -f docker-compose.yml -f docker-compose.staging.yml up -d",
    "start:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "stop:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "stop:staging": "docker-compose -f docker-compose.yml -f docker-compose.staging.yml down",
    "stop:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml down"
  },
  "dependencies": {
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.0.7"
  },
  "packageManager": "yarn@3.8.6+sha512.a1c3a781c248a6d957eacd53f9b965663af89ef777982d7d9086af6de00cab8724cb75bade6b0cdfcaa04f63e20767fc435aade5489be976a5745d6ee03c7932"
}
